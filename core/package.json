{
  "name": "planet",
  "version": "0.0.1",
  "description": "Web 3.0",
  "main": "index.js",
  "author": "Anonymous",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "inspect": "node --experimental-wasm-type-reflection --inspect build/index.js",
    "inspect-brk": "node --experimental-wasm-type-reflection --inspect-brk build/index.js",
    "start": "node --experimental-wasm-type-reflection build/index.js",
    "build:prod": "webpack --mode production",
    "watch:prod": "webpack --mode production --watch",
    "inspect:prod": "node --experimental-wasm-type-reflection --inspect build/bundle.js",
    "inspect-brk:prod": "node --experimental-wasm-type-reflection --inspect-brk build/bundle.js",
    "start:prod": "node --experimental-wasm-type-reflection build/bundle.js",
    "jsdoc-tsd": "jsdoc -t node_modules/libp2p*/jsdoc-tsd -r .",
    "clean": "rimraf ./build",
    "make-tsproto": "cd src && ../node_modules/protoc/protoc/bin/protoc --plugin=../node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=codegen/tsproto --ts_proto_opt=env=node */*/*.proto",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "async-mutex": "^0.2.4",
    "bufferutil": "^4.0.1",
    "cids": "^0.8.3",
    "fast-deep-equal": "^3.1.3",
    "ipfs-bitswap": "^2.0.1",
    "ipfs-repo": "^4.0.0",
    "ipfs-utils": "^2.2.0",
    "ipld-block": "^0.9.2",
    "libp2p": "git+https://github.com/Melotte/js-libp2p.git#v0.28.9-patched",
    "libp2p-bootstrap": "git+https://github.com/Melotte/js-libp2p-bootstrap.git#v0.11.0-patched2",
    "libp2p-floodsub": "^0.21.3",
    "libp2p-gossipsub": "^0.4.6",
    "libp2p-kad-dht": "^0.19.9",
    "libp2p-mdns": "^0.14.2",
    "libp2p-mplex": "^0.9.5",
    "libp2p-noise": "^1.1.2",
    "libp2p-secio": "^0.12.6",
    "libp2p-spdy": "^0.13.3",
    "libp2p-tcp": "^0.14.6",
    "libp2p-websockets": "^0.13.6",
    "mafmt": "git+https://github.com/Melotte/js-mafmt.git#v7.1.0-patched",
    "merge-options": "^2.0.0",
    "multiaddr": "git+https://github.com/Melotte/js-multiaddr.git#v7.5.0-patched",
    "multicodec": "^1.0.4",
    "p-timeout": "^3.2.0",
    "peer-id": "^0.13.13",
    "utf-8-validate": "^5.0.2",
    "wasm-metering": "^0.2.1"
  },
  "devDependencies": {
    "@types/bl": "^2.1.0",
    "@types/chai": "^4.2.11",
    "@types/debug": "^4.1.5",
    "@types/events": "^3.0.0",
    "@types/node": "^14.0.23",
    "patch-package": "^6.2.2",
    "protoc": "^1.0.4",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.1",
    "ts-proto": "^1.26.0",
    "typescript": "^3.9.7",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
